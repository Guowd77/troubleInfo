<view class="content"><view class="username flex  justify-between align-center "><view>{{'您好,'+username+''}}</view><view data-event-opts="{{[['tap',[['logout',['$event']]]]]}}" class="logout" bindtap="__e">退出</view></view><view class="header"><view class="calendar"><image class="cal-img" src="../static/calendar.png" data-event-opts="{{[['tap',[['open',['$event']]]]]}}" bindtap="__e"></image></view><view class="title">{{focusDate}}</view><view data-event-opts="{{[['tap',[['chooseSpace',['$event']]]]]}}" class="user" bindtap="__e">区域</view></view><view class="body"><block wx:for="{{showList}}" wx:for-item="item" wx:for-index="index"><view><view data-event-opts="{{[['tap',[['showModal',[index]]]]]}}" bindtap="__e"><view class="item"><view class="name">{{item.safeRiskName}}</view><view class="address"><image src="../static/address.png"></image>{{''+item.departname}}</view><view class="time"><image src="../static/time.png"></image>{{''+item.newtime}}</view></view></view><view class="liner"></view></view></block><block wx:if="{{modalShow}}"><view class="{{['cu-modal',modalName=='Image'?'show':'']}}"><view class="cu-dialog"><video style="margin-top:20rpx;" id="myVideo" src="https://dcloud-img.oss-cn-hangzhou.aliyuncs.com/guide/uniapp/%E7%AC%AC1%E8%AE%B2%EF%BC%88uni-app%E4%BA%A7%E5%93%81%E4%BB%8B%E7%BB%8D%EF%BC%89-%20DCloud%E5%AE%98%E6%96%B9%E8%A7%86%E9%A2%91%E6%95%99%E7%A8%8B@20181126.mp4"></video><view class="modal"><view class="mname">{{showList[modalNumber].safeRiskName}}</view><view class="maddres"><text class="cuIcon-location text-red "></text>{{''+showList[modalNumber].departname}}</view><view class="mtime"><text class="cuIcon-time text-red"></text>{{''+showList[modalNumber].newtime}}</view></view><view class="mliner"></view><view class="mtext">{{''+showList[modalNumber].saferiskdetails+''}}</view><view class="cu-bar bg-red text-white radius"><view data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="action margin-0 flex-sub  solid-left" bindtap="__e">我知道了</view></view></view></view></block><view class="loadText">{{''+loadText+''}}</view></view><uni-calendar class="vue-ref" vue-id="1" insert="{{false}}" data-ref="calendar" data-event-opts="{{[['^confirm',[['confirm']]]]}}" bind:confirm="__e" bind:__l="__l"></uni-calendar></view>